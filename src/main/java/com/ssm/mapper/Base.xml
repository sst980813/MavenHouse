<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.mapper.BaseMapper">
	<insert id="insertUserTrajectory" parameterType="com.ssm.pojo.UserTrajectory">
		INSERT INTO user_trajectory 
			(
			  trajectory_type,
			  explaindetail,
			  app_id
			)
		VALUES
			(
				#{trajectoryType},
				#{explaindetail},
				#{appID}
			)
	</insert>
</mapper> 
    